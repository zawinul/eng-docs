          "put":
            {
              "tags": ["eng-config-controller"],
              "summary": "Scrive il config di un app",
              "operationId": "setAppConfigUsingPUT",
              "consumes": ["application/json"],
              "produces": ["*/*"],
              "parameters":
                [
                  {
                    "name": "appName",
                    "in": "path",
                    "description": "appName",
                    "required": true,
                    "type": "string"
                  },
